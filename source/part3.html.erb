<% partial 'partials/code_highlight' do %>
  public static double keskiarvo(ArrayList&lt;Integer&gt; luvut) {
      if (luvut.size() == 0) {
          return -1.0;
      }

      int summa = 0;
      for (int luku: luvut) {
          summa = summa + luku;
      }

      return 1.0 * summa / luvut.size();
  }
<% end %>


